#include <windows.h>

#include <string>

int main() {

// Caminho da imagem que será usada como wallpaper

const char* caminholmagem = "C:\Users\Public\Pictures\wallpaper.jpg"; // Coloque sua imagem aqui

// Mudar o wallpaper

System ParametersInfoA(SPI_SETDESKWALLPAPER, 0, (PVOID)caminholmagem, SPIF_UPDATEINIFILE |

SPIF_SENDCHANGE);

// Esconder os ícones da área de trabalho

HWND hProgman = FindWindow("Progman", NULL); ShowWindow(hProgman, SW_HIDE);

// Esconder barra de tarefas

HWND hTaskbar = FindWindow("Shell_TrayWnd", NULL); ShowWindow(hTaskbar, SW_HIDE);

// Manter o programa rodando para que não volte ao normal MessageBox(NULL, "Plano de fundo alterado e área de trabalho oculta!", "Aviso", MB_OK);

// Restaurar área de trabalho e barra ao fechar a mensagem

ShowWindow(hProgman, SW_SHOW); ShowWindow(hTaskbar, SW_SHOW);

return 0;

